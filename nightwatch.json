{
  "src_folders": ["parallel"],
  "output_folder": "reports",

  "selenium": {
    "start_process": true,
    "server_path": "./bin/selenium-server-standalone-3.0.1.jar",
    "log_path": ".",
    "port": 4444,
    "cli_args": {
      "webdriver.chrome.driver": "./bin/chromedriver"
    }
  },

  "test_settings": {
    "default": {
      "desiredCapabilities": {
        "browserName": "chrome",
        "version": "latest",
        "javascriptEnabled": true
      }
    }
  },
  "test_workers" : {
    "enabled" : true, 
    "workers" : 2
  }
}
